<!DOCTYPE html>
<html>
  <head>
    <title>CdpWebManyoTask</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <li id="tasks-index">
            <%= link_to  t('global_navigation.tasks_index'), tasks_path%>
      </li>
      <li id="new-tasks">
            <%= link_to t('global_navigation.new_task'), new_task_path%>
      </li>
    <%= yield %>
  </body>
</html>
