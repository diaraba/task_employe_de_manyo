<h1><%= t('tasks.show.title')  %> </h1>

<p>
  <strong><%= Task.human_attribute_name(:title)%>:</strong>
  <%= @task.title %>
</p>

<p>
  <strong><%= Task.human_attribute_name(:content)%>:</strong>
  <%= @task.content %>
</p>

<p>
  <strong><%= Task.human_attribute_name(:created_at)%>:</strong>
  <%= @task.created_at%>
</p>

<p>
  <strong>expiration_date</strong>
  <%= @task.deadline_on %>
</p>

<p>
  <strong>priority</strong>
  <%= @task.priority %>
</p>

<p>
  <strong>status</strong>
  <%= @task.status %>
</p>

<%= link_to t('common.tasks.edit'), edit_task_path(@task) %> |
<%= link_to t('common.tasks.back'), tasks_path %>
